{
  "name": "suisse-notes-desktop",
  "version": "3.7.1",
  "description": "Desktop app for recording and transcribing meetings",
  "productName": "Suisse Notes",
  "author": "Suisse AI Group GmbH",
  "private": true,
  "scripts": {
    "dev": "quasar dev -m electron",
    "dev:ios": "quasar dev -m capacitor -T ios",
    "dev:android": "quasar dev -m capacitor -T android",
    "build": "quasar build -m electron",
    "build:ios": "quasar build -m capacitor -T ios",
    "build:android": "quasar build -m capacitor -T android",
    "cap:sync": "cd src-capacitor && npx cap sync",
    "cap:open:ios": "cd src-capacitor && npx cap open ios",
    "cap:open:android": "cd src-capacitor && npx cap open android",
    "lint": "eslint --ext .js,.vue ./src",
    "lint:fix": "eslint --ext .js,.vue ./src --fix",
    "prepare": "husky",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:patch": "standard-version --release-as patch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^8.0.1",
    "@capacitor/core": "^8.0.1",
    "@capacitor/device": "^8.0.0",
    "@capacitor/filesystem": "^8.1.0",
    "@capacitor/local-notifications": "^8.0.0",
    "@capacitor/network": "^8.0.0",
    "@capacitor/preferences": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@quasar/extras": "^1.16.9",
    "@sentry/electron": "^5.10.0",
    "axios": "^1.7.9",
    "check-disk-space": "^3.4.0",
    "electron-log": "^5.4.3",
    "electron-store": "^8.1.0",
    "electron-updater": "^6.6.2",
    "fluent-ffmpeg": "^2.1.3",
    "form-data": "^4.0.5",
    "node-machine-id": "^1.1.12",
    "pinia": "^2.1.7",
    "quasar": "^2.17.1",
    "tus-js-client": "^4.3.1",
    "uuid": "^9.0.1",
    "vue": "^3.4.27",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.3.2"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@quasar/app-vite": "^2.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@vue/test-utils": "^2.4.4",
    "autoprefixer": "^10.4.18",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.22.0",
    "husky": "^9.0.0",
    "jsdom": "^24.0.0",
    "lint-staged": "^15.2.0",
    "png2icons": "^2.0.1",
    "sass": "^1.71.1",
    "semantic-release": "^24.2.0",
    "sharp": "^0.34.5",
    "standard-version": "^9.5.0",
    "vite": "^5.1.4",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": "^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.14.12",
    "yarn": ">= 1.22.10"
  }
}
